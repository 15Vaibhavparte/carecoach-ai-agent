{
  "FunctionName": "image-analysis-tool",
  "Runtime": "python3.9",
  "Role": "arn:aws:iam::ACCOUNT_ID:role/lambda-execution-role",
  "Handler": "app.lambda_handler",
  "Code": {
    "ZipFile": "deployment-package.zip"
  },
  "Description": "CareCoach medication identification from image analysis tool",
  "Timeout": 300,
  "MemorySize": 1024,
  "Environment": {
    "Variables": {
      "ENVIRONMENT": "production",
      "AWS_REGION": "us-east-1",
      "BEDROCK_MODEL_ID": "meta.llama3-2-11b-instruct-v1:0",
      "MAX_IMAGE_SIZE": "10485760",
      "MAX_TOKENS": "1000",
      "VISION_TIMEOUT": "30",
      "DRUG_INFO_TIMEOUT": "10",
      "HIGH_CONFIDENCE_THRESHOLD": "0.8",
      "LOW_CONFIDENCE_THRESHOLD": "0.3",
      "LOG_LEVEL": "INFO",
      "DEBUG_MODE": "false"
    }
  },
  "Tags": {
    "Project": "CareCoach",
    "Component": "ImageAnalysis",
    "Environment": "production"
  },
  "DeadLetterConfig": {
    "TargetArn": "arn:aws:sqs:us-east-1:ACCOUNT_ID:image-analysis-dlq"
  },
  "TracingConfig": {
    "Mode": "Active"
  },
  "ReservedConcurrencyLimit": 10,
  "VpcConfig": {
    "SubnetIds": [],
    "SecurityGroupIds": []
  }
}